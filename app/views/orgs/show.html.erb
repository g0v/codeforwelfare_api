<div class="span12">
  <div class="page-header">
    <%= render_ul_breadcrumbs %>
    <h1>機關 - <%= @org.name %></h1>
  </div>
  <dl class="dl-horizontal">
    <dt>英文名稱</dt>
    <dd><%= @org.eng_name %></dd>
    <dt>描述</dt>
    <dd><%= @org.description %></dd>
  </dl>

  <div class="form-actions">
    <%= link_to('返回', orgs_path, :class => 'btn') %>
    <%= link_to('編輯', edit_org_path(@org), :class => 'btn') %>
    <%= link_to('刪除', org_path(@org), :method => 'delete', :confirm => "確定要刪除機關 - #{@org.name}", :class => 'btn btn-danger') %>
  </div>
</div>
