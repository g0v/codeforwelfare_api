<div class="span12">
  <div class="page-header">
    <%= render_ul_breadcrumbs %>
    <h1>
      帳號管理
      <span class="pull-right">
        <%= link_to('新增', new_admin_user_path, :class => 'btn btn-primary') %>
      </span>
    </h1>
  </div>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>編號</th>
        <th>信箱</th>
      </tr>
    </thead>
    <tbody>
      <% @admin_users.each do |admin_user| %>
        <tr>
          <td><%= admin_user.id %></td>
          <td><%= admin_user.email %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= paginate @admin_users, :theme => 'twitter-bootstrap' %>
</div>
